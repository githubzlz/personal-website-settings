<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zlz.website.setting.mapper.UsersMapper">


  <select id="getUsers" resultType="com.zlz.website.settings.common.dos.UserDO">
    SELECT
    *
    FROM users
    <if test="name != null">
      WHERE username like concat('%', #{name}, '%')
    </if>
  </select>
</mapper>